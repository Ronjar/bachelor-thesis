      \strng{namehash}{219220391049affce55f38493007cfdb}
      \strng{fullhash}{219220391049affce55f38493007cfdb}
      \strng{fullhashraw}{219220391049affce55f38493007cfdb}
      \strng{bibnamehash}{219220391049affce55f38493007cfdb}
      \strng{authorbibnamehash}{219220391049affce55f38493007cfdb}
      \strng{authornamehash}{219220391049affce55f38493007cfdb}
