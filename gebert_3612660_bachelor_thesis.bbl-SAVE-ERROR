      \strng{namehash}{e9d490f8b31302bad9a93a92d9a70a51}
      \strng{fullhash}{e9d490f8b31302bad9a93a92d9a70a51}
      \strng{fullhashraw}{e9d490f8b31302bad9a93a92d9a70a51}
      \strng{bibnamehash}{e9d490f8b31302bad9a93a92d9a70a51}
      \strng{authorbibnamehash}{e9d490f8b31302bad9a93a92d9a70a51}
