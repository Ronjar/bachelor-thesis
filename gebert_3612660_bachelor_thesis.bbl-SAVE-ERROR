      \strng{fullhash}{07714e7882dd904fe72d7656b5bd231b}
      \strng{fullhashraw}{07714e7882dd904fe72d7656b5bd231b}
      \strng{bibnamehash}{07714e7882dd904fe72d7656b5bd231b}
      \strng{authorbibnamehash}{07714e7882dd904fe72d7656b5bd231b}
      \strng{authornamehash}{50b28aed2201b3003b3bda80f5b81a0a}
