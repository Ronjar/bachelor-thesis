    \entry{sailerGamificationLearningMetaanalysis2020}{article}{}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=d8d811af5b74abad8cd6b169ab55b1dd}{%
           family={Sailer},
           familyi={S\bibinitperiod},
           given={Michael},
