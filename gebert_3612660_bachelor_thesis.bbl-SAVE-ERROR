      \strng{namehash}{4dca9f56d0719ed4ed9f3280b7c301cc}
      \strng{fullhash}{4dca9f56d0719ed4ed9f3280b7c301cc}
      \strng{fullhashraw}{4dca9f56d0719ed4ed9f3280b7c301cc}
      \strng{bibnamehash}{4dca9f56d0719ed4ed9f3280b7c301cc}
      \strng{authorbibnamehash}{4dca9f56d0719ed4ed9f3280b7c301cc}
      \strng{authornamehash}{4dca9f56d0719ed4ed9f3280b7c301cc}
      \strng{authorfullhash}{4dca9f56d0719ed4ed9f3280b7c301cc}
