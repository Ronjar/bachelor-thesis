      \strng{namehash}{6eb7fbc1f7fa8cfe5511d993ca902051}
      \strng{fullhash}{8f49a1e43df9356da04450d15c57243d}
      \strng{fullhashraw}{8f49a1e43df9356da04450d15c57243d}
      \strng{bibnamehash}{8f49a1e43df9356da04450d15c57243d}
      \strng{authorbibnamehash}{8f49a1e43df9356da04450d15c57243d}
