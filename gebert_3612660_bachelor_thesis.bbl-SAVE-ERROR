    \entry{marteauDevelopmentSixitemShortform1992}{article}{}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=758841d73486de8a783144e8841088c0}{%
           family={Marteau},
           familyi={M\bibinitperiod},
